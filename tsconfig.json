{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./@types", "./node_modules/@types"],
    "lib": ["esnext", "dom.iterable", "scripthost"]
  },
  "extends": "expo/tsconfig.base"
}
