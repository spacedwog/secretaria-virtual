{
  "name": "secretaria-virtual",
  "version": "1.0.0",
  "description": "Este projeto simula uma secretária virtual para um escritório médico. Ele permite o cadastro de pacientes, agendamento de consultas e geração de relatórios em diferentes formatos.",
  "main": "Cloud-dev/server.ts",
  "target": "ESNext",
  "type": "module",
  "scripts": {
    "start": "npx kill-port 3000 && node --loader ts-node/esm Cloud-dev/server.ts",
    "start-clean": "npx kill-port 3000 && node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' Cloud-dev/server.ts",
    "test": "jest",
    "dev": "nodemon Cloud-dev/server.ts",
    "build": "webpack",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --web"
  },
  "ts-node": {
    "loader": "ts-node/esm"
  },
